{
  "cli": {
    "version": ">= 16.24.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "22.21.0"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "node": "22.21.0",
      "preInstall": "npm config set legacy-peer-deps true",
      "postInstall": "npm config set legacy-peer-deps false"
    }
  },
  "submit": {
    "production": {}
  }
}
